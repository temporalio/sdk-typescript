{
  "private": true,
  "name": "@temporalio/test",
  "version": "0.7.0",
  "description": "Temporal.io SDK Tests",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "test": "ava ./lib/test-*.js",
    "test.watch": "ava --watch ./lib/test-*.js"
  },
  "ava": {
    "timeout": "60s",
    "concurrency": 1
  },
  "keywords": [
    "temporal",
    "workflow",
    "worker",
    "isolate"
  ],
  "author": "Roey Berman <roey@temporal.io>",
  "license": "MIT",
  "dependencies": {
    "@temporalio/client": "file:../client",
    "@temporalio/proto": "file:../proto",
    "@temporalio/worker": "file:../worker",
    "@temporalio/workflow": "file:../workflow",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "bugs": {
    "url": "https://github.com/temporalio/sdk-node/issues"
  },
  "homepage": "https://github.com/temporalio/sdk-node#readme"
}
