{
  "private": true,
  "name": "@temporalio/test",
  "version": "1.13.2",
  "description": "Temporal.io SDK Tests",
  "scripts": {
    "build": "npm-run-all build:protos build:ts",
    "build:ts": "tsc --build",
    "build:protos": "node ./scripts/compile-proto.js",
    "test": "ava ./lib/test-*.js",
    "test.watch": "ava --watch ./lib/test-*.js"
  },
  "ava": {
    "timeout": "60s",
    "concurrency": 1,
    "workerThreads": false
  },
  "keywords": [
    "temporal",
    "workflow",
    "worker",
    "isolate"
  ],
