{
  "name": "@temporalio/docs",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "TYPEDOC_WATCH=true DOCUSAURUS_NO_DUPLICATE_TITLE_WARNING=false docusaurus start",
    "build-docs": "TYPEDOC_WATCH=false DOCUSAURUS_NO_DUPLICATE_TITLE_WARNING=false docusaurus build",
    "maybe-install-deps-and-build-docs": "(test -d node_modules || pnpm install --frozen-lockfile) && pnpm run build-docs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/plugin-content-docs": "^3.7.0",
    "@docusaurus/theme-common": "^3.7.0",
    "docusaurus-plugin-snipsync": "^1.0.0",
    "docusaurus-plugin-typedoc": "^0.22.0",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snipsync": "^1.11.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.1"
